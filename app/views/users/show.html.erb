<h1>User Information</h1>
<div class="row">
  <div class="col-md-6 col-xs-6 col-md-offset-3">
		<p id="notice"><%= notice %></p>

		<dl class="dl-horizontal">
		  <dt>Name:</dt>
		  <dd><%= @user.name %></dd>
		</dl>

		<dl class="dl-horizontal">
			<dt>Email:</dt>
			<dd><%= @user.email %></dd>
		</dl>
	</div>
</div>
<div class="center">
	<%= link_to 'back', users_path, class: "position-relative btn btn-default btn-float btn-cancel" %>
</div>